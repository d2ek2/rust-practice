{
    "name": "Rust Development",
    "dockerComposeFile": "../compose.yaml",
    "service": "rust",
    "workspaceFolder": "/workspace",
    "customizations": {
        "vscode": {
            "settings": {
                "terminal.integrated.defaultProfile.linux": "bash",
                "terminal.integrated.profiles.linux": {
                    "bash": {
                        "path": "/bin/bash"
                    }
                }
            }
        }
    },
    "remoteUser": "rustdev"
}